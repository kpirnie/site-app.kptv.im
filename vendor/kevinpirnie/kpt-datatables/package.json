{
    "name": "kpt-datatables-tailwind",
    "version": "1.1.0",
    "description": "Tailwind CSS build for KPT DataTables",
    "private": true,
    "scripts": {
        "build:tailwind": "npx tailwindcss -i ./src/assets/css/themes/tailwind.src.css -o ./src/assets/css/themes/tailwind.css --minify",
        "watch:tailwind": "npx tailwindcss -i ./src/assets/css/themes/tailwind.src.css -o ./src/assets/css/themes/tailwind.css --watch",
        "dev:tailwind": "npx tailwindcss -i ./src/assets/css/themes/tailwind.src.css -o ./src/assets/css/themes/tailwind.css"
    },
    "devDependencies": {
        "tailwindcss": "^3.4.0"
    },
    "keywords": [
        "datatables",
        "tailwind",
        "css"
    ],
    "author": "Kevin Pirnie <me@kpirnie.com>",
    "license": "MIT",
    "dependencies": {
        "npm": "^11.7.0"
    }
}
