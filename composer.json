{
    "name": "kevinpirnie/kp-kptv-filter-app",
    "description": "KPTV Stream Manager - IPTV organization and management system",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Kevin C. Pirnie",
            "email": "iam@kevinpirnie.com"
        }
    ],
    "autoload": {
        "classmap": [
            "controllers"
        ],
        "psr-4": {
            "Kptv\\IptvSync\\": "sync/src/"
        }
    },
    "require": {
        "php": ">=8.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-openssl": "*",
        "kevinpirnie/kpt-datatables": "^1.0",
        "kevinpirnie/kpt-router": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.4"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}